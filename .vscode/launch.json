{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (web:: udap.fhirserver.devdays)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/udap.fhirserver.devdays/bin/Debug/net6.0/udap.fhirserver.devdays.dll",
            "args": [],
            "cwd": "${workspaceFolder}/udap.fhirserver.devdays",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
                "uriFormat": "%s/fhir/r4"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
        {
